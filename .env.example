NODE_ENV=development

# MongoDB
MONGODB_URI=mongodb://localhost:27017/cv-evaluator

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Pinecone
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=us-east-1
PINECONE_INDEX_NAME=cv-evaluator-prod

# Google Generative AI
GOOGLE_GENERATIVE_AI_API_KEY=your_google_api_key


# Embedding Model Configuration
EMBEDDING_MODEL=text-embedding-004
EMBEDDING_DIMENSION=768

# Model Selection
PRIMARY_MODEL=gemini-1.5-pro-latest
FAST_MODEL=gemini-1.5-flash-latest

# Mistral OCR 
MISTRAL_API_KEY=your_mistral_api_key_here
MISTRAL_OCR_MODEL=mistral-ocr-latest

# File Storage
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760  # 10MB

# Job Queue
QUEUE_CONCURRENCY=5
JOB_TIMEOUT=300000      # 5 minutes

# Feature Flags
ENABLE_CALIBRATION_CHECKS=true
ENABLE_MULTI_PASS_EVALUATION=false

# Model Armor
MODEL_ARMOR_ENABLED=true
GCP_PROJECT_ID=project-id
MODEL_ARMOR_LOCATION=gcp-location
MODEL_ARMOR_TEMPLATE_ID=template
GOOGLE_APPLICATION_CREDENTIALS=path-to-secret-accounts